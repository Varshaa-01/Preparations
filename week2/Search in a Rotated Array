https://practice.geeksforgeeks.org/problems/search-in-a-rotated-array4618/1

class Solution
{
    int search(int A[], int l, int h, int key)
    {
        // Complete this function
        for(int i=0;i<h+1;i++){
            if(A[i]==key){
                return i;
            }
        }
        return -1;
    }
}

TIME COMPLEXITY:O(N)
