https://leetcode.com/problems/move-zeroes/

class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int n=nums.size();
        int count=0;
        for(int i=0;i<n;i++){
            if(nums[i]==0){
                count++;
            }
            else{
                nums[i-count]=nums[i];
            }
        }
        int i=n-1;
        while(count--){
            nums[i]=0;
            i--;
        }
    }
};
