https://practice.geeksforgeeks.org/problems/count-substrings0427/1#

class Solution
{
    int countSubstr (String S)
    {
        // your code here  
        int count=0;
        for(int i=0;i<S.length();i++){
            if(S.charAt(i)=='1'){
                count++;
            }
        }
        return ((count*(count-1))/2);
    }
}
